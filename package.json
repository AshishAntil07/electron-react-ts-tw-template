  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "customize-cra": "^1.0.0",
    "electron": "^32.0.1",
    "electron-builder": "^24.13.3",
    "postcss": "^8.4.41",
    "react-app-rewired": "^2.2.1",
    "tailwindcss": "^3.4.10",
    "wait-on": "^8.0.0"
  },
  "build": {
    "appId": "com.example.app",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
